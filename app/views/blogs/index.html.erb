<p style="color: green"><%= notice %></p>

<% content_for :title, "Blogs" %>

<h1>Blogs</h1>

<div id="blogs">
  <% @blogs.each do |blog| %>
    <%= render blog %>
    <p>
      <%= link_to "Show this blog", blog, data: { e2e: "showblog-#{blog.id}" } %>
    </p>
  <% end %>
</div>


<%= link_to "New blog", new_blog_path, data: { e2e: 'newblog' } %>
